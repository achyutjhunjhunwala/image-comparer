{
  "name": "Image-Comparer",
  "version": "1.0.0",
  "description": "API to compare 2 images",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "node dist/index.js",
    "serve": "nodemon lib/index.js --exec babel-node",
